{
  "name": "pilebunker",
  "version": "2.1.0",
  "description": "shiropro weapon calculator pilebunker",
  "private": true,
  "author": "Panepo <panepo@github.io>",
  "homepage": "https://panepo.github.io/PileBunker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Panepo/PileBunker.git"
  },
  "scripts": {
    "parser": "yarn parserC && yarn parserL",
    "parserL": "lsc  ./src/raw/parser.ls --nodejs --harmoy",
    "parserC": "python ./src/raw/charDataParaser.py",
    "start": "react-scripts start",
    "build": "yarn prettier && react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/",
    "prettier": "prettier --single-quote --no-semi --jsx-bracket-same-line --trailing-comma none --write \"src/**/*.{js,jsx}\"",
    "precommit": "lint-staged",
    "predeploy": "yarn parser && yarn build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --single-quote --no-semi --jsx-bracket-same-line --trailing-comma none --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "lokijs": "^1.5.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "flow-bin": "^0.94.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "livescript": "^1.5.0",
    "prettier": "^1.16.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier",
      "flowtype"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
